{
	"name": "any-swap",
	"displayName": "Any Swap",
	"description": "Swap parameters, lines, even complicated expressions from current cursor.",
	"publisher": "wolray",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"homepage": "https://github.com/wolray/vscode-any-swap",
  	"keywords": [
		"swap",
		"transpose",
		"expresions"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/wolray/vscode-any-swap.git"
	},
	"activationEvents": [
		"onCommand:anySwap.forward",
		"onCommand:anySwap.backward"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "anySwap.forward",
				"title": "Any Swap: Forward"
			},
			{
				"command": "anySwap.backward",
				"title": "Any Swap: Backward"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
